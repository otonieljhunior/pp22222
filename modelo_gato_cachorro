import torch
import torch.nn as nn
from torchvision import models

# cria modelo simples sรณ pra gerar o .pth
model = models.resnet18(weights=None)
model.fc = nn.Linear(model.fc.in_features, 2)

# salva na mesma pasta
torch.save(model, "modelo_gato_cachorro.pth")

print("Modelo salvo como modelo_gato_cachorro.pth")

from google.colab import files
files.download("modelo_gato_cachorro.pth")
